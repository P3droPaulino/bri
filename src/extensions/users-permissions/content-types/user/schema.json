{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phoneNumber": {
      "type": "string"
    },
    "phoneNumber_2": {
      "type": "string"
    },
    "birthDate": {
      "type": "date"
    },
    "document_id": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Masculino",
        "Feminino",
        "Outro"
      ]
    },
    "marital_status": {
      "type": "enumeration",
      "enum": [
        "Casado",
        "Solteiro",
        "Divorciado",
        "Viúvo"
      ]
    },
    "motherName": {
      "type": "string"
    },
    "documentNumber": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "fullName": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "street": {
      "type": "string"
    },
    "number": {
      "type": "string"
    },
    "addressComplement": {
      "type": "string"
    },
    "neighborhood": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "document_issue_uf": {
      "type": "string"
    },
    "usernameParent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "active": {
      "type": "boolean"
    },
    "account_status": {
      "type": "enumeration",
      "enum": [
        "Pendente",
        "Análise",
        "Pré-aprovado",
        "Aprovado",
        "Cancelado"
      ]
    },
    "clienteCode_asaas": {
      "type": "string"
    },
    "passwordFinancial": {
      "type": "string"
    },
    "plan_patrocinador": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plan.plan"
    },
    "plan_indicador": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plan.plan"
    }
  }
}
